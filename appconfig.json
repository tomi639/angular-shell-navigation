        ],
        "viewUrl": null,
        "icon": "activity-2",
        "isDefault": false,
        "children": [
          {
            "pathSegment": "edit",
            "labelTranslations": [
              {
                "lang": "en",
                "value": "Edit"
              },
              {
                "lang": "sk",
                "value": "Editovat"
              }
